aryButton.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/SignUpForm.jsx",["240"],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/LoginForm.jsx",["241"],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/reducers/user.js",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/pages/styling/PageContainer.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/lib/IconHeart.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/LoginDrawer.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/pages/ShopPage.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/pages/ProductPage.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/pages/GalleryPage.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/pages/ContactPage.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/pages/CartPage.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/pages/StartPage.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/reducers/ui.js",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/styling/StyledNav.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/styling/HeaderContainer.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/lib/HeaderLogo.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/lib/StartPageLogo.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/pages/styling/ProductPageContainer.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/lib/IconArrow.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/lib/IconCart.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/pages/ProfilePage.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/CartDrawer.jsx",["242","243","244"],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/reducers/cart.js",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/TotalCartItems.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/styling/StyledDrawer.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/pages/styling/ShopCard.jsx",["245"],"import React, { useState } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport styled from \"styled-components/macro\";\nimport { Link } from \"react-router-dom\";\nimport { handleLikeThunk } from \"../../reducers/user\";\n\nimport { IconHeart } from \"../../lib/IconHeart\";\nimport { Dialog } from \"../../lib/Dialog\";\n// import { useTooltip } from \"../../lib/Tooltip\";\n\nconst Container = styled.article`\n  position: relative;\n  background: #fff;\n  padding: 10px;\n  margin-bottom: 20px;\n\n  cursor: pointer;\n\n  & p {\n    margin: 0;\n    color: #6b6b6b;\n  }\n\n  & h3 {\n    margin: 0;\n    color: #000;\n    text-transform: lowercase;\n    font-size: 32px;\n  }\n\n  &:hover h3 {\n    text-decoration: underline;\n  }\n\n  & img {\n    width: 100%;\n  }\n\n  &:hover img {\n    opacity: 0.7;\n    transition: 0.1s ease-out;\n  }\n\n  @media (max-width: 667px) {\n    &:hover img {\n    opacity: 1;\n  }\n\n  }\n`;\n\nconst TitleBar = styled.div`\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n`;\n\nconst Content = styled.div`\n  padding: 10px 0;\n`;\n\nconst Button = styled.button`\n  background: none;\n  border: none;\n  padding: 0;\n  outline: none;\n  cursor: pointer;\n`;\n\nconst Heart = styled(IconHeart)`\n  padding-top: 6px;\n  transition: 0.1s;\n  & .cls-1 {\n    fill: ${(props) => props.liked && \"#000\"};\n  }\n  & .cls-2 {\n    fill: ${(props) => props.liked && \"#000\"};\n  }\n`;\n\nconst TextFlag = styled.span`\n  position: absolute;\n  z-index: 5;\n  right: 0;\n  text-align: right;\n  margin-top: 12px;\n\n  margin-right: 10px;\n  padding: 3px 10px 4px 10px;\n  background: ${(props) => (props.sample ? \"#fff\" : \"#000\")};\n  color: ${(props) => (props.sample ? \"#000\" : \"#fff\")};\n  font-size: 18px;\n  font-style: italic;\n`;\n\nconst StyledLink = styled(Link)`\n  text-decoration: none;\n`;\n\nexport const ShopCard = (props) => {\n  const { title, price, coverImage, className, quantity, id, sample } = props;\n\n  const [showLikeAlert, setShowLikeAlert] = useState(false);\n\n  const accessToken = useSelector((store) => store.user.login.accessToken);\n  const likes = useSelector((store) => store.user.likes);\n\n  const dispatch = useDispatch();\n\n  const handleLike = (id) => {\n    if (accessToken) {\n      dispatch(handleLikeThunk(id));\n    } else {\n      setShowLikeAlert(true);\n    }\n\n    console.log(\"Liked\", id);\n  };\n\n  // const ref = useTooltip(\"sign in to like\", {});\n\n  return (\n    <>\n      {showLikeAlert && (\n        <Dialog\n          title=\"Sign in to like\"\n          buttonText=\"ok\"\n          onClose={() => setShowLikeAlert(false)}\n        />\n      )}\n      <Container className={className}>\n        {quantity < 1 && <TextFlag>sold out</TextFlag>}\n        {sample && <TextFlag sample>sample</TextFlag>}\n\n        <StyledLink to={`/product/${id}`}>\n          <img src={coverImage} />\n        </StyledLink>\n\n        <Content>\n          <TitleBar>\n            <StyledLink to={`/product/${id}`}>\n              <h3>{title}</h3>\n            </StyledLink>\n            <Button>\n              <Heart\n                liked={likes?.includes(id)}\n                onClick={() => handleLike(id)}\n              />\n            </Button>\n          </TitleBar>\n          <p>{`${price} SEK`}</p>\n        </Content>\n      </Container>\n    </>\n  );\n};\n","/Users/jonnaru/Technigo/final-project/final-project/frontend/src/pages/styling/