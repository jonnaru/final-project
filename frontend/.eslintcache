 {\n    if (flag) {\n      container = document.createElement(\"div\");\n      document.body.append(container);\n      const { top, left } = current.getBoundingClientRect();\n      const pos = {\n        top: top + current.offsetHeight + shift,\n        left: left + current.offsetWidth / 2,\n      };\n      ReactDOM.render(<Tooltip {...pos}>{message}</Tooltip>, container);\n    } else {\n      if (container) {\n        container.remove();\n      }\n    }\n  };\n\n  React.useEffect(() => {\n    const current = ref.current;\n    if (current) {\n      current.addEventListener(\"mouseover\", () => hoverHandler(current, true));\n      current.addEventListener(\"mouseout\", () => hoverHandler(current, false));\n    }\n    return () => {\n      if (container) {\n        container.remove();\n      }\n    };\n  }, []);\n\n  return ref;\n};\n","/Users/jonnaru/Technigo/final-project/final-project/frontend/src/pages/styling/ContactPageContainer.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/Footer.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/styling/FooterContainer.jsx",[],{"ruleId":"229","replacedBy":"230"},{"ruleId":"231","replacedBy":"232"},{"ruleId":"233","severity":1,"message":"234","line":40,"column":6,"nodeType":"235","endLine":40,"endColumn":8,"suggestions":"236"},{"ruleId":"233","severity":1,"message":"237","line":38,"column":6,"nodeType":"235","endLine":38,"endColumn":8,"suggestions":"238"},{"ruleId":"239","severity":1,"message":"240","line":6,"column":10,"nodeType":"241","messageId":"242","endLine":6,"endColumn":14},{"ruleId":"239","severity":1,"message":"243","line":33,"column":9,"nodeType":"241","messageId":"242","endLine":33,"endColumn":20},{"ruleId":"239","severity":1,"message":"244","line":50,"column":9,"nodeType":"241","messageId":"242","endLine":50,"endColumn":23},{"ruleId":"245","severity":1,"message":"246","line":129,"column":11,"nodeType":"247","endLine":129,"endColumn":35},{"rule