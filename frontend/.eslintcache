e = (id) => {\n    if (accessToken) {\n      dispatch(handleLikeThunk(id));\n    } else {\n      setShowLikeAlert(true);\n    }\n\n    console.log(\"Liked\", id);\n  };\n\n  // const ref = useTooltip(\"sign in to like\", {});\n\n  return (\n    <>\n      {showLikeAlert && (\n        <Dialog\n          title=\"Sign in to like\"\n          buttonText=\"ok\"\n          onClose={() => setShowLikeAlert(false)}\n        />\n      )}\n      <Container className={className}>\n        {quantity < 1 && <TextFlag>sold out</TextFlag>}\n        {sample && <TextFlag sample>sample</TextFlag>}\n\n        <StyledLink to={`/product/${id}`}>\n          <img src={coverImage} />\n        </StyledLink>\n\n        <Content>\n          <TitleBar>\n            <StyledLink to={`/product/${id}`}>\n              <h3>{title}</h3>\n            </StyledLink>\n            <Button>\n              <Heart\n                liked={likes.includes(id)}\n                onClick={() => handleLike(id)}\n              />\n            </Button>\n          </TitleBar>\n          <p>{`${price} SEK`}</p>\n        </Content>\n      </Container>\n    </>\n  );\n};\n","/Users/jonnaru/Technigo/final-project/final-project/frontend/src/pages/styling/ProductCard.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/styling/CartList.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/styling/CartItemCard.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/lib/IconMinus.jsx",[],["231","232"],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/DropDown.jsx",["233"],"import React, { useState } from \"react\";\nimport styled from \"styled-components/macro\";\n\nimport { Backdrop } from \"./styling/Backdrop\";\n\nconst DropDownContainer = styled.div`\n  position: absolute;\n  top: 70px;\n\n  width: 130px;\n  padding-left: 10px;\n  font-size: 14px;\n  font-family: Arial, Helvetica, sans-serif;\n  text-transform: uppe