: center;\n  justify-content: space-between;\n`;\n\nconst CoverImage = styled.img`\n  width: 100%;\n  /* padding: 0 10px; */\n\n  &:hover {\n    opacity: 0.7;\n  }\n`;\n\nconst Content = styled.div`\n  padding: 10px 0;\n`;\n\nconst ChildrenContent = styled.div`\n  background: #f1f1f1;\n  padding: 20px;\n`;\n\nconst Heart = styled(IconHeart)`\n  z-index: 15;\n  &:hover path {\n    fill: #000;\n  }\n`;\n\nconst TextFlag = styled.p`\n  position: absolute;\n  z-index: 5;\n  right: 0;\n  text-align: right;\n  margin-top: 12px;\n\n  margin-right: 10px;\n  padding: 3px 10px 4px 10px;\n  background: ${(props) => (props.sample ? \"#fff\" : \"#303030\")};\n  color: ${(props) => (props.sample ? \"#000\" : \"#fff\")};\n  font-size: 18px;\n  font-style: italic;\n`;\n\nconst StyledLink = styled(Link)`\n  text-decoration: none;\n\n  &:hover h3 {\n    text-decoration: underline;\n  }\n`;\n\nexport const ShopCard = ({\n  title,\n  secondaryText,\n  coverImage,\n  className,\n  quantity,\n  id,\n  sample,\n}) => (\n  <Container className={className}>\n    {quantity < 1 && <TextFlag>sold out</TextFlag>}\n    {sample && <TextFlag sample>sample</TextFlag>}\n\n    <StyledLink to={`/product/${id}`}>\n      <CoverImage src={coverImage} />\n\n      <Content>\n        <TitleBar>\n          <div>\n            <Title>{title}</Title>\n            <SecondaryText>{`${secondaryText} SEK`}</SecondaryText>\n          </div>\n          <Heart />\n        </TitleBar>\n      </Content>\n    </StyledLink>\n  </Container>\n);\n","/Users/jonnaru/Technigo/final-project/final-project/frontend/src/lib/ProductCard.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/reducers/ui.js",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/styling/StyledNav.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/styling/HeaderContainer.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/lib/HeaderLogo.jsx",[],"/Users/