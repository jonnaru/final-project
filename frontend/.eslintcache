nt(\"div\");\n      document.body.append(container);\n      const { top, left } = current.getBoundingClientRect();\n      const pos = {\n        top: top + current.offsetHeight + shift,\n        left: left + current.offsetWidth / 2,\n      };\n      ReactDOM.render(<Tooltip {...pos}>{message}</Tooltip>, container);\n    } else {\n      if (container) {\n        container.remove();\n      }\n    }\n  };\n\n  React.useEffect(() => {\n    const current = ref.current;\n    if (current) {\n      current.addEventListener(\"mouseover\", () => hoverHandler(current, true));\n      current.addEventListener(\"mouseout\", () => hoverHandler(current, false));\n    }\n    return () => {\n      if (container) {\n        container.remove();\n      }\n    };\n  }, []);\n\n  return ref;\n};\n",["248","249"],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/pages/styling/ContactPageContainer.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/Footer.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/styling/FooterContainer.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/pages/styling/ProfileItemsContainer.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/pages/styling/ProfilePageContainer.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/pages/styling/ProductPageImage.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/styling/HeaderContentContainer.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/styling/StyledTotalItems.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/lib/Backdrop.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/styling/StyledCartCard.jsx",[],"/Users/jonnaru/Technigo/final-project/final-project/frontend/src/components/styling/HeaderCartContainer.jsx",[],"/Users/jonnaru/Technigo/final-